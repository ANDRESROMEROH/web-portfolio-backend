{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "max-len": [
            "error",
            120
        ],
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "import/no-unresolved": "off",
        "no-console": "off",
        "import/extensions": "off",
        "no-return-assign": "off",
        "camelcase": "off",
        "no-underscore-dangle": "off",
        "operator-linebreak": "off",
        "no-extra-semi": "off"
    }
}